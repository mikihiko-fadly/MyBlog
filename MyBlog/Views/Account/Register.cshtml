@model User


<h1>Registrasi</h1>
<form method="post" action="Register">
	<label>Name</label>
	<input type="text" asp-for="Name"
		   class="form-control" />
	<div>
		<span asp-validation-for="Name" class="text-danger"></span>
	</div>

	<label>Username</label>
	<input type="text" asp-for="Username"
		   class="form-control" />
	<div>
		<span asp-validation-for="Username" class="text-danger"></span>
	</div>

	<label>Email</label>
	<input type="email" asp-for="Email"
		   class="form-control" />
	<div>
		<span asp-validation-for="Email" class="text-danger"></span>
	</div>

	<label>Password</label>
	<input type="password" asp-for="Password"
		   class="form-control" />
	<div>
		<span asp-validation-for="Password" class="text-danger"></span>
	</div>

	<label>Address</label>
	<input type="text" asp-for="Alamat"
		   class="form-control" />
	<div>
		<span asp-validation-for="Alamat" class="text-danger"></span>
	</div>

	<label>Phone</label>
	<input type="text" asp-for="NoTelepon"
		   class="form-control" />
	<div>
		<span asp-validation-for="NoTelepon" class="text-danger"></span>
	</div>

	<label>Date of Birth</label>
	<input type="datetime-local" asp-for="TanggalLahir"
		   class="form-control" />
	<div>
		<span asp-validation-for="TanggalLahir" class="text-danger"></span>
	</div>

	<button type="submit" class="btn btn-primary">
		Save
	</button>
	<a href="/Account/Show" class="btn btn-danger">User Table</a>
</form>